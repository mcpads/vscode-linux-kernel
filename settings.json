{
    "files.exclude": {
        "**/.*.*.cmd": true,
        "**/.*.d": true,
        "**/*.o": true,
        "**/*.a": true,
        "**/alpha/**": true,
        "**/arc/**": true,
        "**/arm/**": true,
        "**/arm64/**": true,
        "**/csky/**": true,
        "**/hexagon/**": true,
        "**/loongarch/**": true,
        "**/m68k/**": true,
        "**/mips/**": true,
        "**/microblaze/**": true,
        "**/nios2/**": true,
        "**/openrisc/**": true,
        "**/powerpc/**": true,
        "**/parisc/**": true,
        "**/riscv/**": true,
        "**/s390/**": true,
        "**/sparc/**": true,
        "**/xtensa/**": true
    },
    "[c]": {
        "editor.detectIndentation": false,
        "editor.tabSize": 8,
        "editor.insertSpaces": false,
        "editor.rulers": [80,100]
    },
    "files.associations": {
        "*.h": "c"
    },
    "C_Cpp.default.compilerPath": "gcc"
}
